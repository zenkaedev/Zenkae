{
  "name": "zenkae-bot",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": { "node": ">=18.17" },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",

    "postinstall": "prisma generate",
    "start": "prisma db push --accept-data-loss && tsx src/index.ts",
    "start:migrate": "prisma migrate deploy && tsx src/index.ts",

    "deploy:global": "tsx scripts/deploy-commands.ts",
    "clear:guild": "tsx scripts/clear-guild-commands.ts",

    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed"
  },
  "prisma": { "seed": "tsx prisma/seed.ts" },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@sentry/node": "^10.5.0",
    "discord.js": "^14.21.1",
    "dotenv": "^16.4.5",
    "pino": "^9.9.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/pino": "^7.0.4",
    "prisma": "^5.17.0",
    "tslib": "^2.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}

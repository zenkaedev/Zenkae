{
  "name": "zenkae-bot",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "postinstall": "prisma generate",
    "start": "prisma generate && prisma migrate deploy && tsx src/index.ts",
    "start:migrate": "prisma generate && prisma migrate deploy && tsx src/index.ts",
    "deploy:global": "tsx scripts/deploy-commands.ts",
    "clear:guild": "tsx scripts/clear-guild-commands.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@resvg/resvg-js": "^2.6.2",
    "@sentry/node": "^10.5.0",
    "discord-api-types": "^0.38.34",
    "discord.js": "^14.25.1",
    "dotenv": "^16.4.5",
    "node-cron": "^4.2.1",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.3",
    "react": "^19.2.3",
    "satori": "^0.18.3",
    "tsx": "^4.20.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.11",
    "@types/pino": "^7.0.4",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "prisma": "^5.17.0",
    "tslib": "^2.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.47.0"
  }
}
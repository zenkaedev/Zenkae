{
  "name": "zenkae",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": { "node": ">=20.18.0" },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "deploy": "npm run deploy:dev",
    "emojis:sync": "tsx scripts/emojis.sync.ts",
    "deploy:dev": "tsx scripts/commands.ts publish-guild",
    "deploy:global": "tsx scripts/commands.ts publish-global",
    "deploy:list:dev": "tsx scripts/commands.ts list-guild",
    "deploy:list:global": "tsx scripts/commands.ts list-global",
    "deploy:wipe:dev": "tsx scripts/commands.ts wipe-guild",
    "deploy:wipe:global": "tsx scripts/commands.ts wipe-global",
    "prepare": "husky install",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev --name update",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@sentry/node": "^8.0.0",
    "discord.js": "^14.16.3",
    "pino": "^8.21.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.0",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "tsx": "^4.16.0",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": ["eslint --fix", "prettier --write"],
    "*.{js,json,md}": ["prettier --write"]
  }
}
